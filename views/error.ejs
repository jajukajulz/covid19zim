<!DOCTYPE html>
<html lang="en">
  <head>
        <%- include ('./partials/head') %>
  </head>
  <body>

    <!-- START: header -->
    <header>
      <%- include ('./partials/header')  %>
    </header>
    <!-- END: header -->

    <main role="main">
      <div class="album py-5 bg-light">
        <div class="container">
          <div class="row align-items-center justify-content-center text-wrap text-break" id="notificationBarRow">
              <div class="col-md-8">
                <%- include ('./partials/notificationbar') %>
              </div>
          </div>
          <div class="row align-items-center justify-content-center" id="errorRow">
            <div>
              <h5>
                <p class="text-muted">ERROR <i class="fas fa-exclamation-circle"></i></p>
              </h5>
            </div>
            <div class = "container">
              <div class="row">
                <h1><%= message %></h1>
                <h2><%= error.status %></h2>
                <pre><%= error.stack %></pre>              
              </div>
              <p><a href="/">Go to Home Page</a></p>
          </div>
          </div> 
        </div>
      </div>
    </main>
    <!-- start footer -->
    <%- include ('./partials/footer') %>
    <!-- end footer -->
  </body>
</html>

